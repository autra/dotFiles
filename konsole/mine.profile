[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Kantix2
Font=FiraCode Nerd Font,10,-1,5,50,0,0,0,0,0
UseFontLineChararacters=false

[General]
Command=/bin/zsh
DimWhenInactive=false
Environment=TERM=xterm-256color
Name=mine
Parent=FALLBACK/

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=2
ScrollBarPosition=2

[Terminal Features]
FlowControlEnabled=false
